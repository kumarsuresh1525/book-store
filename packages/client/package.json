    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "babel-loader": "^9.1.2",
    "html-webpack-plugin": "^5.5.0",
    "eslint": "8.13.0",
