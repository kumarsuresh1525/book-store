    "eslint-plugin-react": "7.29.0",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
